}
.body{
  margin-top:30rpx;
}
.row{
  width: 90%;
  height:100rpx;
  border-bottom:1px solid #eee;
  margin:0 auto;
  /* margin-bottom:30rpx; */
  background: #fff;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 30rpx;
  /* font-family:"grace-iconfont"; */
  /* border-radius: 5rpx; */
}
.info{
  width:100%;
  padding-left:30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between
}
.info .left{
  color:#8f8f8f;
}
.info .right{
 font-size: 35rpx;
 color:#8f8f8f;
}
.grace-list{
  padding:none;
  border:none;
  /* padding-left:60rpx;
  padding-right:60rpx; */
}
.grace-accordion-items{
 width: 90%;
 margin:0 auto;
 border:none;
}
.grace-accordion-title{
  color:#8f8f8f;
  font-size: 28rpx;
  padding-left:30rpx;
  border-bottom:1px solid #eee;
  height: 100rpx;
  line-height: 100rpx;
  display: flex;
  align-items: center;
  width:95%;
  padding:none;
  font-family:none;

}
.grace-list .items .title{
  /* padding-left: 30rpx; */
  width: 100%;
  font-size: 28rpx;
  color:#8f8f8f;
  border-bottom:1px solid #eee;
  font-family:none;
}
.grace-list .items .title:last-child{
  border-bottom:1px solid #eee !important;
}
.radio-group{
  width: 100%;
  height: 100rpx;
  /* border-bottom:1px solid #eee; */
}
.radio checkbox{
  transform:scale(0.7,0.7)
}
