/* 位置定位 */
.section{
  width:90%;
  margin:0 auto;
  display: flex;
  /* align-items: center; */
  /* justify-content: space-between; */
  border-bottom:1px solid #eee;
}
picker{
  width: 100%;
}
.pos{
  width: 70%;
  display: flex;
  align-items: center;
}
.picker{
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
  /* border-bottom:1px solid #eee; */
  font-size: 30rpx;
  color:#7F7F7F;
}
.picker view{
  margin-left:20rpx;
	color:#818288;
}
/* 详细地址 */
.address{
  width: 90%;
  margin:0 auto;
  padding-top:20rpx;
  border:1px solid #eee;
  margin-top:20rpx;
  /* border-bottom:1px solid #eee;
  border-left:1px solid #eee;
  border-right:1px solid #eee; */
}
.address textarea{
  line-height: 20rpx;
  width: 95%;
  margin:0 auto;
}

.btn{
  width:250rpx;
  height: 70rpx;
  line-height: 70rpx; 
  background: #feb70f;
  text-align: center;
  color:#fff;
  margin-right: 30rpx;
  border-radius: 10rpx;
}
.submit{
  width: 90%;
  margin:0 auto;
  margin-top:100rpx;
  margin-bottom:50rpx;
  height: 100rpx;
  background:#feb70f;
  color:#fff; 
}