/* 设备优势 */
.photoAdv{
  display: flex;
  background: #eee;
  margin-bottom:30rpx;
}
.photoAdv .advView{
  width:48%;
  height: 300rpx;
}
.photoAdv .img{
  width: 48%;
  height: 300rpx;
  box-sizing: border-box;
  border:1px solid #bfbfbf;
  position: relative;
}
.photoAdv .img image{
  position: absolute;
  left:50%;
  top:50%;
  transform: translate(-50%,-50%);
}
.photoAdv .advText{
  width: 46%;
  margin-left:10rpx;
  height: 270rpx;
 overflow: hidden;
}
.photoAdv .advText{
  width: 50%;
}
.photoAdv .advText .title{
  height: 50rpx;
  line-height: 50rpx;
  font-size: 35rpx;
}
.photoAdv .advText .des{
  font-size: 30rpx;
  height: 270rpx;
  overflow: hidden;
 white-space:normal !important;
  display: -webkit-box;
 -webkit-box-orient: vertical;
 -webkit-line-clamp:5;
}
.photoAdv .advText .des textarea{
  line-height: 40rpx;
}
