/* 上传照片 */
.Upload{
  width: 100%;
  height: 150rpx;
  line-height: 150rpx;
  margin: 0 auto;
  display:flex;
  align-items: center;
  justify-content: start;
  display: hidden; 
}
.Upload>image{
  margin-left:20rpx; 
}
.Upload>image:nth-child(5){
  margin-right: 20rpx;
}
.photoBox{
  width: 120rpx;
  height: 120rpx;
  border:1px solid #bfbfbf;
  margin-left:20rpx;
  position: relative;
}
.photoBox1{
  display: none
}
.photoBox>image{
  position: absolute;
  left:50%;
  top:50%;
  transform: translate(-50%,-50%);
}
.goodsName{
  width: 100%;
  display: flex;
  align-items: center;
  height: 100rpx;
  /* border-top:1px solid #eee; */
  border-bottom:1px solid #eee;
  /* color:#06C2F6 */
}
.goodsName .title text:nth-child(1){
  width:50%;
  color:#06C2F6
}
.goodsName .title text:nth-child(2){
  color:red;
}
.goodsName input{
  width: 50%;
  height: 30rpx;
  line-height: 30rpx;
  margin-left:20rpx;
}
