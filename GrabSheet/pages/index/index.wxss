.body{
  /* background: #eee; */
}
/* 头部开始 */
.head{
  width: 100%;
  position: relative;
}
.posSearch{
  width: 100%;
  position: absolute;
  top:2rpx;
  z-index: 10;
  color:#fff;
  font-size: 28rpx;
  display: flex;
  align-items: center;
  justify-content:left;
}
.position{
  width:30%;
  display: flex;
  align-items: center;
}
.search{
  margin-top:15rpx;
  width:70%;
  color:#bfbfbf;
  background: rgba(255,255,255,0.8);
  border-radius: 18rpx;
  margin-right: 10rpx;
  display: flex;
  align-items: center;
  justify-content: center
}
/* 头部结束 */
.pic{
  width: 95%;
  margin: 0 auto;
  background: #fff;
  margin-top:15rpx;
}
/* 下单用户 */
.orderUser{
  width:100%;
  border: 1px solid #eee;
  height:100rpx;
  display: flex;
  align-items: center;
  justify-content: flex-start;
}
.notice{
  display: flex;
  flex-direction: column;
  font-size: 25rpx;
  margin-left:40rpx;
  width: 100rpx;
  text-align: center;
  color:#E96F1E;
}
/* 字幕滚动 */
.swiper_container {
  height: 50rpx; 
  line-height: 50rpx;
  width: 90vw; 
}
.swiper_item { 
  font-size: 30rpx; 
  overflow: hidden; 
  text-overflow: ellipsis; 
  white-space: nowrap;
  }
  
/* 下单用户结束 */
/* 附近服务商 */
.chosen{
  width: 95%;
  margin: 0 auto;
}
.chosen>.title{
  display: flex;
  justify-content: space-between;
  height: 100rpx;
  line-height: 100rpx;
  color:#06C2F6;
  font-weight: 800;
  font-size: 35rpx;
}
.chosen .title .title1{
  font-size: 25rpx;
  color:#E96F1E;
  font-weight: 800
}
.chosen .img{
  width: 100%; 
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  justify-content: space-around;
}
.chosen .nav{
  width:48%;
  margin-bottom:10rpx;  
}
.chosen .nav .title{
  width:95%;
  margin: 0 auto;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
  height: 50rpx;
  line-height: 50rpx;
  /* display: flex; */
  justify-content: center;
  color:#000;
  font-size: 28rpx;
  font-weight:400;
}
/* 招标展示 */
/* page/myOrder/myOrder.wxss */
 /* Tab横向切换 */
.info{
  width: 95%;
  margin: 0 auto;
  height: 60rpx;
  line-height: 60rpx;
  box-sizing: border-box;
  border:1px solid #06C2F6;
  border-radius:10rpx; 
  display: flex;
  font-size: 30rpx;
}
.default{
  width: 50%;
  text-align: center;
  /* color:#fff; */
  /* border-radius:30rpx; */
}
.active{
  width:50%;
  background: #06C2F6;
  text-align: center;
  color:#fff;
  /* border-radius:30rpx; */
}
.show{
  display:block;
  text-align:center;
  /* padding-bottom:20rpx; */
  /* margin-bottom: 20rpx; */
  /* border-bottom:1px solid #eee; */
  /* line-height:200rpx; */
}
.hidden{
  display:none;
  text-align:center;
  /* line-height:200px; */
}
.show .nav{
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  border-bottom:1px solid #eee;
}
.show navigator:last-child{
  /* border-bottom:none */
}
.listAll{
  width: 93%;
  margin: 0 auto;
  /* height: 50rpx;
  line-height: 50rpx; */
  display: flex;
  /* margin-bottom: 20rpx; */
  justify-content: space-between;
}
.leftText{
  font-size: 30rpx;
  width: 80%;
  overflow: hidden;
  text-align: left;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.iconRight{
  color:#bfbfbf;
}
.contact{
  width:100%;
  background: #eee;
  display: flex;
  flex-direction: column;
  justify-content: center;
  font-size: 30rpx;
  text-align: center;
  line-height: 50rpx;
  color:#707070;
  padding-top:10rpx;
  padding-bottom:30rpx;
}
/* 更多 */
.more .text{
  width: 90%;
  margin: 0 auto;
  background: #eee;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 30rpx;
  text-align: center;
  margin-top:20rpx;
  margin-bottom:20rpx;
  border:1px solid #bfbfbf;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}


